{
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "vim.normalModeKeyBindings": [
    ],
    "vim.visualModeKeyBindings": [
    ],
    "vim.operatorPendingModeKeyBindings": [
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {"before": ["s"], "after": ["<Leader>", "<Leader>", "2", "s", "<char>", "<char>"]},
        {"before": ["<Leader>"], "after": ["<Leader>", "<Leader>"]},
        {"before": ["<Tab>"],"commands" : ["editor.action.indentLines"]}, 
        {"before": ["<S-Tab>"],"commands" : ["editor.action.outdentLines"]},
        {"before": ["<ESC>", "<ESC>"], "commands": [":noh"]},
        {"before": ["<C-[>", "<C-[>"], "commands": [":noh"]}
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {"before": ["j", "j"],"after" : ["<Esc>"]}, 
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {"before": ["<Leader>"], "after": ["<Leader>", "<Leader>"]},
        {"before": ["<Tab>"],"commands" : ["editor.action.indentLines"]}, 
        {"before": ["<S-Tab>"],"commands" : ["editor.action.outdentLines"]}, 
    ],
    "vim.operatorPendingModeKeyBindingsNonRecursive": [
        {"before": ["<Leader>"], "after": ["<Leader>", "<Leader>"]},
    ],
    "vim.useCtrlKeys": true,
    "vim.overrideCopy": true,
    "vim.cursorStylePerMode.normal" : "block",
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.cursorStylePerMode.replace": "block-outline",
    // "vim.statusBarColorControl": true,
    // "vim.statusBarColors.normal": ["#A4FF96", "#000000"],
    // "vim.statusBarColors.insert": ["#00D1FF", "#000000"],
    // "vim.statusBarColors.visual": ["#F09996", "#000000"],
    // "vim.statusBarColors.visualline": ["#F09996", "#000000"],
    // "vim.statusBarColors.visualblock": ["#F09996", "#000000"],
    // "vim.statusBarColors.replace": ["#E2A478", "#000000"],
    // "vim.statusBarColors.commandlineinprogress": ["#E0E0E0", "#000000"],
    // "vim.statusBarColors.searchinprogressmode": ["#E0E0E0", "#000000"],
    // "vim.statusBarColors.easymotionmode": ["#E0E0E0", "#000000"],
    // "vim.statusBarColors.easymotioninputmode": ["#E0E0E0", "#000000"],
    // "vim.statusBarColors.surroundinputmode": ["#E0E0E0", "#000000"],
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.debug.silent": true,
    "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
    "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
    "vim.autoSwitchInputMethod.defaultIM": "com.google.inputmethod.Japanese.Roman",
    "vim.autoSwitchInputMethod.enable": true,
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.easymotionDimBackground": false,
    "vim.easymotionMarkerBackgroundColor": "#AAAAAA",
    "vim.easymotionMarkerForegroundColorOneChar": "#000000",
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "#552222",
    "vim.easymotionMarkerForegroundColorTwoCharSecond": "#662222",
    "vim.easymotionMarkerFontWeight": "normal",
    // "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",

    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "on",
    "editor.minimap.renderCharacters": false,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "boundary",
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Myrica M, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.lineHeight": 18,
    "editor.suggestSelection": "first",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.cursorSurroundingLines": 9,

    "explorer.compactFolders": false,
    "explorer.sortOrder": "modified",
    "explorer.confirmDelete": false,

    "files.autoGuessEncoding": true,

    "todo-tree.tree.showScanModeButton": false,

    "terminal.external.osxExec": "iTerm.app",
    "terminal.explorerKind": "external",
    "terminal.integrated.fontSize": 15,

    "workbench.editor.enablePreview": false,
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.list.keyboardNavigation": "filter",
    "workbench.iconTheme": "material-icon-theme",

    "markdown.preview.fontSize": 15,

    "git.confirmSync": false,

    "macros": {
        "viewExplorerAndToggleSidebar": [
            "workbench.view.explorer",
            "workbench.action.toggleSidebarVisibility"
        ],
        "listParentandCollapse": [
            "list.focusParent",
            "list.collapse"
        ]
    },

    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

    "liveServer.settings.port": 0,
    "liveServer.settings.CustomBrowser": "chrome",
    "editor.renderControlCharacters": true,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Monokai",
    "editor.detectIndentation": false,

}